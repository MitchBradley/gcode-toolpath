    "chai": "^3.4.1",
    "coveralls": "^2.11.4",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0",
    "mocha": "^2.3.4"
  }
